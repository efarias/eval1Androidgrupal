<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HistorialPagos">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Historial De Viajes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="414dp"
        android:layout_height="561dp"
        android:layout_marginTop="104dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <Button
            android:id="@+id/btnViajesMetro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="historialMetro"
            android:text="Viajes en Metro" />

        <Button
            android:id="@+id/btnViajesTaxi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="historialTaxi"
            android:text="Viajes en Taxi" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>